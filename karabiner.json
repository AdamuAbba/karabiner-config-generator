    {
      "name": "Default",
      "complex_modifications": {
        "rules": [
          {
            "description": "Hyper Key (⌃⌥⇧⌘)",
            "manipulators": [
              {
                "from": {
                  "key_code": "caps_lock"
                },
                "to": [
                  {
                    "key_code": "left_shift",
                    "modifiers": [
                      "left_command",
                      "left_control",
                      "left_option"
                    ]
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Hyper Key sublayer: O (\"Open\")",
            "manipulators": [
              {
                "description": "Toggle Hyper sublayer o",
                "type": "basic",
                "from": {
                  "key_code": "o",
                  "modifiers": {
                    "mandatory": [
                      "left_command",
                      "left_control",
                      "left_shift",
                      "left_option"
                    ]
                  }
                },
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "hyper_sublayer_o",
                      "value": false
                    }
                  }
                ],
                "to": [
                  {
                    "set_variable": {
                      "name": "hyper_sublayer_o",
                      "value": true
                    }
                  }
                ]
              },
              {
                "to": [
                  {
                    "shell_command": "open -a 'Google Chrome.app'"
                  }
                ],
                "description": "Open Google Chrome",
                "type": "basic",
                "from": {
                  "key_code": "g",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "hyper_sublayer_o",
                    "value": true
                  }
                ]
              },
